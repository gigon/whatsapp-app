{
    "manifest_version": 3,
    "name": "Open in WhatsApp App",
    "version": "1.1",
    "description": "Create a message to the selected number in the WhatsApp Windows App",
    "permissions": ["contextMenus", "tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  